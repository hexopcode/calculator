# UNIT TESTS FOR REFERENCES
ASSERT(!ISDEF($A))
A=1
ASSERT(ISDEF($A))
ASSERT(TYPE($A) == "REF")
ASSERT(RESOLVE($A) == 1)
ASSERT(!ISFROZEN($A))
FREEZE($A)
ASSERT(ISFROZEN($A))
DELETE($A)
ASSERT(!ISDEF($A))
ASSERT(ASSIGN($A, 1) == 1)
ASSERT(ISDEF($A))
ASSERT(A == 1)
DELETE($A)

